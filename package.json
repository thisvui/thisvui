{
  "main": "dist/thisvui.js",
  "name": "thisvui",
  "version": "0.0.1-alpha.2",
  "description": "A Vue.js + Bulma Components library",
  "author": "ncanatta <ncanatta@gmail.com>",
  "scripts": {
    "build-dev": "webpack --config build/webpack.lib.config.js",
    "build-prod": "webpack --config build/webpack.lib.config.js --env.production --progress",
    "postbuild-dev": "webpack --config build/webpack.styles.config.js",
    "postbuild-prod": "webpack --config build/webpack.styles.config.js --env.production --progress",
    "lint": "vue-cli-service lint",
    "pack": "npm pack",
    "showversion": "echo \"Version: $npm_package_version\""
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "bulma": "^0.7.2",
    "bulma-checkradio": "^2.1.0",
    "bulma-switch": "^2.0.0",
    "date-fns": "^1.30.1",
    "flatpickr": "^4.5.2"
  },
  "peerDependencies": {
    "vue": "2.5.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime-corejs2": "^7.3.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.0.0-0",
    "file-loader": "^3.0.1",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^0.3.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.3",
    "vue": "^2.6.7",
    "vue-loader": "^15.6.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.6",
    "webpack-clean": "^1.2.3",
    "webpack-cli": "^3.2.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "warning"
    }
  }
}

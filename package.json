{
  "name": "thisvui",
  "version": "0.1.0-beta.6",
  "description": "A Vue.js Components library",
  "author": "ncanatta <ncanatta@gmail.com>",
  "scripts": {
    "build": "webpack --config build/webpack.lib.config.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "analize": "webpack --config build/webpack.lib.config.js --profile --json > webpack-stats.json",
    "build:prod": "npm run build -- --env.production --progress",
    "bundle": "npm run build && npm run build:prod",
    "pack": "npm pack",
    "postbuild": "webpack --config build/webpack.styles.config.js",
    "postbuild:prod": "webpack --config build/webpack.styles.config.js --env.production --progress",
    "showversion": "echo \"Version: $npm_package_version\""
  },
  "main": "dist/thisvui.js",
  "module": "dist/thisvui.js",
  "unpkg": "dist/thisvui.js",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "bulma": "^0.7.5",
    "date-fns": "^2.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime-corejs2": "^7.8.7",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^2.1.0",
    "eslint-config-prettier": "^4.1.0",
    "file-loader": "^3.0.1",
    "jest": "^25.2.4",
    "jest-serializer-vue": "^0.3.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.5.0",
    "minireset.css": "0.0.6",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.3",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-clean": "^1.2.3",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "vue": "2.6.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "warning"
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "homepage": "http://www.thisvui.com",
  "jsdelivr": "dist/thisvui.js",
  "license": "MIT",
  "repository": "thisvui/thisvui",
  "sideEffects": [
    "*.css"
  ]
}
